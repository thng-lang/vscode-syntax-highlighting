{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "thng-lang",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#statements"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"statements": {
			"patterns": [{
				"name": "keyword.control.thng",
				"match": "\\b(if|other|else|loop|res)\\b"
			}]
		},
		"operators": {
			"patterns": [
				{
					"match": "([\\+\\-\\*\\^\/])",
					"name": "keyword.operators.arithmetic.thng"
				},
				{
					"match": "([\\"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.thng",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.thng",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.thng"
}